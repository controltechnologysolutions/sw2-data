					"protocol": "https",
					"host": [
						"maps",
						"googleapis",
						"com"
					],
					"path": [
						"maps",
						"api",
						"geocode",
						"json"
					],
					"query": [
						{
							"key": "address",
							"value": "2530+Arnold+Drive,MARTINEZ,CA,94553"
						},
						{
							"key": "key",
							"value": "{{api_key}}"
						}
					]
				}
			},
			"response": []
		}
	],
